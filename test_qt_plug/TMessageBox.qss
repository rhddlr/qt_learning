QMessageBox{
	background-color:#353535;
}
QPushButton{
	color: rgba(255, 255, 255, 217);
	min-width: 78px;
	min-height: 22px;
	background-color:rgba(255, 255, 255, 0.1);
	border-radius: 4px;
	padding: 3px 12px 3px 12px
}
QPushButton:hover{
    background-color:rgba(255, 255, 255, 0.2)
}
*[timedButtonField="true"]
{
	font:12px;
	color: rgba(255, 255, 255, 217);
}
*[titleField="true"]{
	font:12px;
	color: rgba(255, 255, 255, 217);
}
*[textField="true"]{
	font:12px;
	color: rgba(255, 255, 255, 166);
}
*[solutionField="true"]{
	font:12px;
	color: rgba(255, 255, 255, 102);
}
*[closeIconButton="true"]{
	min-width:16px;
	min-height:16px;
	max-width:16px;
	max-height:16px;
	image: url(./style/ChromeClose.png);
	border-radius: 0px;
	padding: 0px;
	background-color:transparent;
}